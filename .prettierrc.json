{
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "arrowParens": "avoid",
    "importOrder": ["^react(.*)", "<THIRD_PARTY_MODULES>", "@/(.*)", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }